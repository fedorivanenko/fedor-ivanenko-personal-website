{
  "name": "fedor-personal-website",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --filter=web",
    "build": "turbo run build",
    "start": "turbo run start --filter=web",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "@types/react": "19.2.6",
    "@types/react-dom": "19.2.3",
    "tsup": "^8.0.0",
    "turbo": "^2.1.3",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@9.0.0",
  "pnpm": {
    "overrides": {
      "esbuild": "npm:esbuild-wasm@0.17.19",
      "@types/react": "19.2.6",
      "@types/react-dom": "19.2.3"
    }
  }
}
